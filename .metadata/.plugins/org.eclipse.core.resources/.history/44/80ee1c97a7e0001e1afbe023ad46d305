package linklist;
//Node 
class Nodal{
	int data;
	Nodal next;
	Nodal header=null;
	
	
	
	public void add(int x)
	{
		Nodal n=new Nodal();
		if(header==null)
		{
			n.data=x;
			n.next=null;
			header=n;
		}
		
		
		
	}
}

//Linked List class
public class LinkedList{
	
	
	public static void main(String[] args) {
	Nodal node=new Nodal();
	node.add(34);
	node.add(45);
	System.out.println(node.data);
		
	}
}